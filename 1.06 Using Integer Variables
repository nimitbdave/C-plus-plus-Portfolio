#include <iostream>
using namespace std; 

int main()
{ // prints "m = 44 and n = 77":
  int m, n;
  m = 44;         // assigns the value 44 to the variable m
  cout << "m = " << m;
  n = m + 33;     // assigns the value 77 to the variable n
  cout << " and n = " << n << endl;
}

//  Output:  
//  m = 44 and n = 77
