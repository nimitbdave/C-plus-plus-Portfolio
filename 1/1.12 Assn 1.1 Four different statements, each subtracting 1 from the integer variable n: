// Write four different C++ statements, each subtracting 1 from the integer variable n.

#include <iostream>
using namespace std; 

int main()
{ // Four different statements, each subtracting 1 from the integer variable n:
  cout << n = n - 1;
  cout << n -= 1;
  cout << --n;
  cout << n--;
}

