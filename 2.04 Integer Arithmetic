  
/* 

*/

#include <iostream>
using namespace std; 

int main()
{ // tests operators +,-,*,/,and %:
    
    int m=54;
    int n=20;
    
    cout << "m = "    << m << " and n = " << n << endl;
    
    cout << "m+n = "  << m+n << endl; // 54+20 = 74
    cout << "m-n = "  << m-n << endl; // 54-20 = 34
    cout << "m*n = "  << m*n << endl; // 54*20 = 1080
    cout << "m/n = "  << m/n << endl; // 54/20 = 2
    cout << "m%n = "  << m%n << endl; // 54%20 = 14
}

/*
OUPUT: 
m = 54 and n = 20
m+n = 74
m-n = 34
m*n = 1080
m/n = 2
m%n = 14
*/  
