/* 
Write and run a program that prints the ASCII codes
for only the 10 upper case and lower case vowels. 
*/

#include <iostream>
using namespace std; 

int main()
{ // prints the ASCII codes of the vowels

      cout << "int('A') = " << int('A') << endl;
      cout << "int('E') = " << int('E') << endl;
      cout << "int('I') = " << int('I') << endl;
      cout << "int('O') = " << int('O') << endl;
      cout << "int('U') = " << int('U') << endl;
      
      cout << "int('a') = " << int('a') << endl;
      cout << "int('e') = " << int('e') << endl;
      cout << "int('i') = " << int('i') << endl;
      cout << "int('o') = " << int('o') << endl;
      cout << "int('u') = " << int('u') << endl;
}

/*
OUPUT: 
int('A') = 65
int('E') = 69
int('I') = 73
int('O') = 79
int('U') = 85
int('a') = 97
int('e') = 101
int('i') = 105
int('o') = 111
int('u') = 117
*/
