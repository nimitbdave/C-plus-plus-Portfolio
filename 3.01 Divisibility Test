/* 
This program tests if one positive integer is not divisible by another: 
*/

#include <cfloat>
#include <cmath>
#include <iostream>
using namespace std; 

int main()
{   
    int n,d;
    
    cout << "Enter two positive integers: ";
    cin >> n >> d;  //Input numerator and denominator
    
    if (n%d)    //If there is a remainder, then this becomes a non-zero condition, allowing the process which follows.
        cout << n << " is not divisible by " << d << endl;
}

/*
OUPUT: 
Enter two positive integers: 66 7
66 is not divisible by 7
*/
