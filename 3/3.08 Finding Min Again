/* 

*/

#include <cfloat>
#include <cmath>
#include <iostream>
using namespace std; 

int main()
{   
    int n1,n2,n3;
    
    cout << "Enter three integers: ";
    cin >> n1 >> n2 >> n3;
    
    if (n1 <= n2 && n1 <= n3) cout << "Their minimum is " << n1 <<endl;
    if (n2 <= n1 && n2 <= n3) cout << "Their minimum is " << n2 <<endl;
    if (n3 <= n1 && n3 <= n2) cout << "Their minimum is " << n3 <<endl;
}

/*
OUPUT: 
Enter two integers: 77 33 55
Their minimum is 33
*/
