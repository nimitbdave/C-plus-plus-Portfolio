/* 

*/

#include <cfloat>
#include <cmath>
#include <iostream>
using namespace std; 

int main()
{ int n,i=0;
  cout << "Enter a positive integer: ";
  cin >> n;
  
  long sum=0;
  do
    sum += i++;
  while (i <= n);
  cout << "The sum of the first " << n << " integers is " << sum;
}

