/* 

*/

#include <cfloat>
#include <cmath>
#include <iostream>
using namespace std; 

int main()
{ long bound;
  
  cout << "Enter a positive integer: ";
  cin >> bound;
  
  cout << "Factorial numbers that are <= " << bound << ":\n1,1";
  
  long f=1;
  for (int i=2; f <= bound; i++)
  { f *= i;
    cout << "," << f;
  }
}

/*
OUPUT: 
Enter a positive integer: 1000000
Factorial numbers < 1000000:
1,1,2,6,24,120, 720,5040,40320,362880
*/
