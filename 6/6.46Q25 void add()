#include <cfloat>
#include <iomanip> // defines setw()
#include <cmath> // defines pow() and log(), fabs(), sqrt()
#include <iostream> // defines cin and cout
using namespace std; 

int main()
{ // 
    
}
void add(float a[], int n, float b[])
{ for (int i=0; i<n; i++)
    a[i] += b[i];
}

