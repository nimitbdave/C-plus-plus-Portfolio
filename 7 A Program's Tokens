#include <iostream>
using namespace std; 

int main()
{ // prints "n = 44":
int n=44;
cout << "n = " << n << endl;
}

//  Output:  
//  n = 44

//This source code has 19 tokens: “int”, “main”, “(”, “)”, “{”, “int”, “n”, “=”, “44”, “;”, “cout”,
//“<<”, “"n = "”, “<<”, “n”, “<<”, “endl”, “;”, and “}”. Note that the compiler ignores the comment
//symbol // and the text that follows it on the second line.
